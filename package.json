{
  "name": "delete-me-vim",
  "publisher": "alfonsekon",
  "displayName": "Delete Me! Vim",
  "description": "Delete Me! Vim is an extension that aims to train your Vim motions, relative line jumping and line deletion in particular.",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/alfonsekon/delete-me-vim"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "delete-me-vim.relative-line-jump",
        "title": "Delete Me! Vim - Relative Line Jump"
      },
      {
        "command": "delete-me-vim.maze",
        "title":  "Delete Me! Vim - Maze"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.21.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
